CREATE TABLE PROFILE(
    ID int NOT NULL AUTO_INCREMENT,
    NAME varchar(50) NOT NULL,
    CONSTRAINT pk1 PRIMARY KEY(ID)
);
    
CREATE TABLE USUARIO(
    ID int NOT NULL AUTO_INCREMENT,
    PROFILEID int,
    FIRST_NAME varchar(62),
    LAST_NAME varchar(62),
    DNI varchar(8),
CONSTRAINT pk2 PRIMARY KEY(ID),
    CONSTRAINT pk3 FOREIGN KEY(PROFILEID) REFERENCES PROFILE(ID)
);

CREATE TABLE CAMPUS(
    ID int NOT NULL AUTO_INCREMENT,
    NAME varchar(62) NOT NULL,
    ADDRESS varchar(98),
    STATE varchar(80),
    CITY varchar(43),
    CONSTRAINT pk4 PRIMARY KEY(ID)
    );

CREATE TABLE DEGREE(
    ID int NOT NULL AUTO_INCREMENT,
    NAME varchar(62) NOT NULL,
    CONSTRAINT pk18 PRIMARY KEY(ID)
);

CREATE TABLE USERCAMPUSDEGREE(
    ID int NOT NULL AUTO_INCREMENT,
    USERID int,
    CAMPUSID int,
    DEGREEID int,
    CONSTRAINT pk5 PRIMARY KEY(ID),
    CONSTRAINT pk6 FOREIGN KEY(USERID) REFERENCES USUARIO(ID),
    CONSTRAINT pk7 FOREIGN KEY(CAMPUSID) REFERENCES CAMPUS(ID),
    CONSTRAINT pk8 FOREIGN KEY(DEGREEID) REFERENCES DEGREE(ID)
    );
    
CREATE TABLE CURRICULUM(
    ID int NOT NULL AUTO_INCREMENT,
    DEGREEID int,
    CONSTRAINT pk9 PRIMARY KEY(ID),
    CONSTRAINT pk10 FOREIGN KEY(DEGREEID) REFERENCES DEGREE(ID)
    );

CREATE TABLE SUBJECT(
    ID int NOT NULL AUTO_INCREMENT,
    NAME varchar(62),
    CONSTRAINT pk11 PRIMARY KEY(ID)
);

CREATE TABLE SUBJECTCORRELATIVE(
    ID int NOT NULL AUTO_INCREMENT,
    SUBJECTID int,
    CORRELATIVEID int,
    CONSTRAINT pk12 PRIMARY KEY(ID),
    CONSTRAINT pk13 FOREIGN KEY(SUBJECTID) REFERENCES SUBJECT(ID),
    CONSTRAINT pk14 FOREIGN KEY(CORRELATIVEID) REFERENCES SUBJECT(ID)
    );

CREATE TABLE CURRICULUMSUBJECT(
ID int AUTO_INCREMENT,
    SUBJECTID int,
    CURRICULUMID int,
    CONSTRAINT pk15 PRIMARY KEY(ID),
    CONSTRAINT pk16 FOREIGN KEY(SUBJECTID) REFERENCES SUBJECT(ID),
    CONSTRAINT pk17 FOREIGN KEY(CURRICULUMID) REFERENCES CURRICULUM(ID)
);
